{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
        "config:base"
    ],
    "customManagers": [
        {
            "customType": "regex",
            "managerFilePatterns": [
                "ivy.xml"
            ],
            "matchStrings": [
                "<dependency\\s+org=\"(?<packageName>[^\"]+?)\"\\s+name=\"(?<depName>[^\"]+?)\"\\s+rev=\"(?<currentValue>[^\"]+?)\"[^/>]*/>"
            ],
            "datasourceTemplate": "maven"
        }
    ]
}
